    "@types/glob": "^8.0.0",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^16.11.65",
    "dtsgenerator": "^3.8.0",
    "glob": "^8.0.3",
    "jose": "^4.10.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
